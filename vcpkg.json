{
  "name": "mystlviewer",
  "version": "1.0.0",
  "description": "Modern OpenGL STL file viewer using SDL3",
  "dependencies": [
    "sdl3",
    "glad",
    "glm",
    "nativefiledialog-extended",
    "earcut-hpp",
    {
      "name": "pugixml",
      "features": ["compact"]
    },
    {
      "name": "libarchive",
      "features": ["zstd", "lzma"]
    }
  ]
}
